<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

<item android:state_pressed="false">
	<layer-list>
		<item>
			<shape android:shape="rectangle">
			<stroke
				android:width="1dp"
				android:color="@color/ykyin_white_pass"
				/>
			<solid
				android:color="@color/ykyin_white"
				/>
				<corners
					android:radius="3dp"/>
			</shape>
		</item>
	</layer-list>
</item>

<item>
	<layer-list>
		<item>
			<shape android:shape="rectangle">
				<stroke
					android:width="1dp"
					android:color="@color/ykyin_white_pass"
					/>

				<solid
					android:color="@color/ykyin_white_pass"
					/>
				<corners
					android:radius="3dp"/>
			</shape>
		</item>
	</layer-list>
</item>

</selector>
