<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:state_pressed="false">
	    <layer-list>
	        <item>
	            <shape android:shape="rectangle"> 
	                <stroke
	                    android:width="1dp"
	                    android:color="@color/ykyin_blue_normal"
	                    />
	                 <solid
	                     android:color="@color/ykyin_blue_normal"
	                     />   
	                  <corners
						  android:radius="180dp"
	                      />   
	            </shape>    
	        </item>    
	    </layer-list>    
	</item>

	<item>
	    <layer-list>
	        <item>
	            <shape android:shape="rectangle"> 
	                <stroke
	                    android:width="1dp"
	                    android:color="@color/ykyin_blue_pass"
	                    />
	                 <solid
	                     android:color="@color/ykyin_blue_pass"
	                     />
					<corners
						android:radius="180dp"
						/>
				</shape>
	        </item>    
	    </layer-list>    
	</item>   
	
	   
	
</selector>